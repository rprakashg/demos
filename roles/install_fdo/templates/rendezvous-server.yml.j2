storage_driver:
  {%- if use_postgresql %}
  Postgres:
    server: Rendezvous 
    url: "{{ rendezvous_db_url }}"
  {%- else %}
  Directory:
    path: /path/to/stores/rendezvous_registered
  {%- endif %}
session_store_driver:
  Directory:
    path: /etc/fdo/stores/rendezvous_sessions
trusted_manufacturer_keys_path: /etc/fdo/keys/manufacturer_cert.pem
max_wait_seconds: ~
bind: "0.0.0.0:{{ rendezvous.port.host_port | default('8082') }}"